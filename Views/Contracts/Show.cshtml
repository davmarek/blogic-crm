@model Contract

@{
    ViewData["Title"] = "Clients";
}
<h2>Contract</h2>

<table class="table">
    <thead>
    <tr>
        <th>@Html.DisplayNameForInnerType((Contract i) => i.Client)</th>
        <th>@Html.DisplayNameForInnerType((Contract i) => i.Created)</th>
        <th>@Html.DisplayNameForInnerType((Contract i) => i.Effective)</th>
        <th>@Html.DisplayNameForInnerType((Contract i) => i.Closed)</th>
        <th>@Html.DisplayNameForInnerType((Contract i) => i.Institution)</th>
    </tr>
    </thead>

    <tbody>
    
            <tr>
                <td>@($"{Model.Client.FirstName} {Model.Client.LastName}")</td>
                <td>@Html.DisplayFor(model => Model.Created)</td>
                <td>@Html.DisplayFor(model => Model.Effective)</td>
                <td>@Html.DisplayFor(model => Model.Closed)</td>
                <td>@Model.Institution.Name</td>
            </tr>
    </tbody>
</table>